        lcd.print("C");
        lcd.setCursor(4,0);
        lcd.blink();
         
       delay(1000);
        timediff=0;
      }
}
}
      
      if(digitalRead(button1)==0&& j==2 && buttonpushed==-1&&m==0 )
      {
       lcd.setCursor(5,0);
       lcd.blink();
         Serial.println("Cursor6");
        j=3;
        delay(1000);
      }
      
       if(ITS1>=32 && ITS1<=212 && ITS2>=32 && ITS2<=212 && o==1 && p==0)
{  
      if(timediff>=100 && timediff<=1000)
{
        if( j==3 && buttonpushed==-1 &&m==0)
