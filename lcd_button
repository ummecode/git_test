unsigned long event=3000;
int buttonpushed=0;

#include <LiquidCrystal.h>
#include <Preferences.h>
Preferences preferences;
const int rs=12,rw=14,en=27,d4=26,d5=25,d6=33,d7=32;
LiquidCrystal lcd(rs,rw,en,d4,d5,d6,d7);
#define button1 5
#define button2 17
const int analogPin1=34;
const int analogPin2=35;
const int analogPin3=36;
  const int analogPin4=39;

const int Relay=15;
const int Pump=2;
int i=98;

int j;
int m;
int k=118;
int l=(5*(i-32))/9;
int n=(5*(k-32))/9;
int o,p;

unsigned long timediff;
unsigned long presstime,releasetime;
bool flag1=0;
 int val1,val2,val3,val4,ITS1,ITS2;
float VM1,VM11,VM2,VM22,V1,V2;
int ITS1C,ITS2C; 
int MF=10;



void setup() {
  Serial.begin(9600);
  preferences.begin("gpio",false);
  digitalWrite(Relay, LOW);
  digitalWrite(Pump, LOW);
  digitalWrite(rw,LOW);
  pinMode(Relay, OUTPUT);
  pinMode(Pump, OUTPUT);
  pinMode(button1,INPUT);
  pinMode(button2,INPUT);
int read_value1=preferences.getInt("state1",i);
int read_value2=preferences.getInt("state2",k);
int read_value3=preferences.getInt("state3",l);
int read_value4=preferences.getInt("state4",n);
Serial.println(read_value1);
Serial.println(read_value2);
Serial.println(read_value3);
Serial.println(read_value4);
i=read_value1;
k=read_value2;
l=read_value3;
n=read_value4;
  lcd.begin(8,1);
 lcd.clear();
}

void loop() {
  
   if((digitalRead(button1)==1 && digitalRead(button2)==1 && buttonpushed==0 )||(digitalRead(button1)==1 && digitalRead(button2)==0 && buttonpushed==0 )||(digitalRead(button1)==0 && digitalRead(button2)==1 && buttonpushed==0 )){
  lcd.clear();
/*val3=analogRead(analogPin3);
VM1=val3*3.3/4095;
Serial.print("VM1+: ");
Serial.println( VM1,4);

val4=analogRead(analogPin4);
VM11=val4*3.3/4095;
Serial.print("VM1-: ");
Serial.println( VM11,4);

V1=(VM1)-(VM11);
V2=(VM2)-(VM22);*/

val1=analogRead(analogPin1);
V1=val1*3.3/(4095*MF);
  ITS1C=(4*V1/0.00385)/(3.3-2*V1);
  
  ITS1=(9*ITS1C/5)+32;
  
  Serial.print("V1:");
  Serial.println( V1,4);
  Serial.print("ITS1C: ");
  Serial.print( ITS1C);
  Serial.println( "C");
  Serial.print(" ITS1 :");
  Serial.print( ITS1);
  Serial.println( "F");
   Serial.println();
    if( (timediff==0) || (timediff>=100 && timediff<=1500)){
      lcd.clear();
  lcd.print("T1:");
  lcd.print(ITS1);
  lcd.print("F");
  o=1;
  p=0;
    }
  if( timediff>2000){
    lcd.clear();
    lcd.print("T1:");
  lcd.print(ITS1C);
  lcd.print("C");
  p=1;
  o=0;
  
    }
    
  
  if( o==1 && p==0){
   
   if(ITS1>=i && ITS2<=k) {
    digitalWrite(Relay,HIGH);
  }
  else{
    digitalWrite(Relay,LOW);
  }
   }
    else if( p==1 && o==0){
     
    if(ITS1C>=l && ITS1C<=n) {
    digitalWrite(Relay,HIGH);
  }
  else{
    digitalWrite(Relay,LOW);
  }
   }

   M1();
   M2();
  delay(1000);
   M1();
   M2();
 delay(1000);
   M1();
   M2();
  
 val2=analogRead(analogPin2);
  V2=val2*3.3/(4095*MF);
  ITS2C=(4*V2/0.00385)/(3.3-2*V2);
  ITS2=(9*ITS2C/5)+32;
  Serial.print("  V2:");
  Serial.println( V2);
   Serial.print("ITS2C: ");
  Serial.print( ITS2C);
  Serial.println( "C");
   Serial.print(" ITS2 :");
  Serial.print( ITS2);
   Serial.println( "F");
   lcd.setCursor(0,1);
  if( (timediff==0) || (timediff>=100 && timediff<=1500)){
    lcd.clear();
  lcd.print("T2:");
  lcd.print(ITS2);
  lcd.print("F");
  o=1;
  p=0;
    }
 
  if( timediff>2000){
    lcd.clear();
    lcd.print("T2:");
  lcd.print(ITS2C);
  lcd.print("C");
  p=1;
  o=0;
    }
   if( o==1 && p==0){
    
   if(ITS2>=i && ITS2<=k) {
    digitalWrite(Pump,HIGH);
  }
  else{
    digitalWrite(Pump,LOW);
  }
   }
    else if( p==1 && o==0){
     
   if(ITS2C>=l && ITS2C<=n) {
    digitalWrite(Pump,HIGH);
  }
  else{
    digitalWrite(Pump,LOW);
  }
   }
    M1();
   M2();
  delay(1000);
   M1();
   M2();
 delay(1000);
  M1();
   M2();
  
 if(digitalRead(Relay)==1 && buttonpushed==0)
  {
    lcd.clear();
 lcd.print("RELAY ON");
   M1();
   M2();
 delay(1000);
  M1();
   M2();
 delay(1000);
 M1();
   M2();
 }
 if(digitalRead(Relay)==0 && buttonpushed==0)
 {
   lcd.clear();
  lcd.print("RELAYOFF");
   M1();
   M2();
   delay(1000);
    M1();
   M2();
 delay(1000);
 M1();
   M2();
 }
 
  if(digitalRead(Pump)==1&& buttonpushed==0)
  {
 lcd.clear();
 lcd.print("PUMP ON");
 M1();
   M2();
 delay(1000);
  M1();
   M2();
 delay(1000);
  M1();
   M2();
 }
  if(digitalRead(Pump)==0&& buttonpushed==0)
 {
 lcd.clear();
 lcd.print("PUMP OFF");
 M1();
   M2();
 delay(1000);
 M1();
   M2();
 delay(1000);
 M1();
   M2();
 }
 
  }
  
  Serial.println(millis());
  //delay(1000);
  
if(digitalRead(button1)==0 && buttonpushed==0)
  {
    buttonpushed=millis();
    Serial.print("buttonpushed:");
    Serial.println(buttonpushed);
   }
   
if(ITS1>=32 && ITS1<=212 && ITS2>=32 && ITS2<=212 && o==1 && p==0)
{
    if(digitalRead(button1)==0 && buttonpushed>0)
    {
    unsigned long difference=millis()-buttonpushed;
    if(difference>=event)
    {
      lcd.clear();
      Serial.print("difference:");
      Serial.println(difference);
     
     Serial.print("B1:");
     Serial.print(i);
      Serial.println("F");
      if(i>=0 && i<=9){
        lcd.print("B1:  ");
          }
         else if(i>=10 && i<=99){
        lcd.print("B1: ");
          }
         else{
          lcd.print("B1:");
        }
      lcd.print(i);
      lcd.print("F");
       lcd.setCursor(3,0);
      lcd.blink();
     
      Serial.println("Cursor4");
      j=1;
      m=0;
     buttonpushed=-1;
     Serial.print("buttonpushed:");
    Serial.println(buttonpushed);
    timediff=0;
      delay(3000);
    }
    }
}
    if(ITS1C>=0 && ITS1C<=100 && ITS2C>=0 && ITS2C<=100 && p==1 && o==0)
{
    if((digitalRead(button1)==0 && buttonpushed>0)|| (digitalRead(button1)==1&& buttonpushed>0))
    {
    unsigned long difference=millis()-buttonpushed;
    if(difference>=event)
    {
      lcd.clear();
      Serial.print("difference:");
      Serial.println(difference);
     
     Serial.print("B1:");
     Serial.print(l);
      Serial.println("C");
      if(l>=0 && l<=9){
        lcd.print("B1:  ");
          }
         else if(l>=10 && l<=99){
        lcd.print("B1: ");
          }
         else{
          lcd.print("B1:");
        }
      lcd.print(l);
      lcd.print("C");
       lcd.setCursor(3,0);
      lcd.blink();
      
      Serial.println("Cursor4");
      j=1;
      m=0;
     buttonpushed=-1;
     Serial.print("buttonpushed:");
    Serial.println(buttonpushed);
    timediff=0;
      delay(3000);
    }
    }
}

     if(digitalRead(button2)==0 && flag1==0)
        {
          presstime=millis();
          flag1=1;
        Serial.print("presstime:");
    Serial.println(presstime);
        }
        
        if(digitalRead(button2)==1&& flag1==1)
        {
          releasetime=millis();
          flag1=0;
         
          Serial.print("releasetime:");
    Serial.println(releasetime);
    timediff=releasetime-presstime;
    Serial.print("timediff:");
    Serial.println(timediff);
        }
  
if(ITS1>=32 && ITS1<=212 && ITS2>=32 && ITS2<=212 && o==1 && p==0)
{
if(timediff>=100 && timediff<=1000)
{
      if( j==1 && buttonpushed==-1 && m==0)
      {
        lcd.clear();
        i=i+100;
        if(i>299 && i<=500){
          i=0;
        }
       Serial.print("B1:");
        Serial.print(i);
         Serial.println("F");
         if(i>=0 && i<=9){
         lcd.print("B1:  ");
          }
         else if(i>=10 && i<=99){
        lcd.print("B1: ");
          }
         else{
          lcd.print("B1:");
        }
        lcd.print(i);
        lcd.print("F");
        lcd.setCursor(3,0);
      lcd.blink();
        
        delay(1000);
        timediff=0;
        
      }
}
}
if(ITS1C>=0 && ITS1C<=100 && ITS2C>=0 && ITS2C<=100 && p==1&& o==0)
{
  if(timediff>=100 && timediff<=1000)
{
      if( j==1 && buttonpushed==-1 && m==0)
      {
        lcd.clear();
        l=l+100;
        if(l>100){
          l=0;
        }
       Serial.print("B1:");
        Serial.print(l);
         Serial.println("C");
         if(l>=0 && l<=9){
         lcd.print("B1:  ");
          }
         else if(l>=10 && l<=99){
        lcd.print("B1: ");
          }
         else{
          lcd.print("B1:");
        }
        lcd.print(l);
        lcd.print("C");
        lcd.setCursor(3,0);
      lcd.blink();
        
        delay(1000);
        timediff=0;
        
      }
}
}

      
      
      if(digitalRead(button1)==0 && j==1 && buttonpushed==-1&&m==0 )
      {
        lcd.setCursor(4,0);
        lcd.blink();
     Serial.println("Cursor5");
        j=2;
       delay(1000);
      }
    if(ITS1>=32 && ITS1<=212 && ITS2>=32 && ITS2<=212 && o==1 && p==0)
{  
      if(timediff>=100 && timediff<=1000)
{
        if( j==2 && buttonpushed==-1 && m==0)
      {
        lcd.clear();
        i=i+10;
        if(i>299 && i<=500){
          i=0;
        }
       Serial.print("B1:");
        Serial.print(i);
        Serial.println("F");
