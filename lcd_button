{  
     if( timediff==0 && j==5 && k>i){
      lcd.clear();
     Serial.print("B1:");
     Serial.print(i);
     Serial.println("F");
     if(i>=0 && i<=9){
        lcd.print("B1:  ");
          }
        else if(i>=10 && i<=99){
        lcd.print("B1: ");
          }
         else{
          lcd.print("B1:");
        }
        lcd.print(i);
  lcd.print("F");
  preferences.putInt("state1",i); 
  
  delay(1000);
  lcd.clear();
     Serial.print("B2:");
     Serial.print(k);
     Serial.println("F");
    if(k>=0 && k<=9){
        lcd.print("B2:  ");
          }
        else if(k>=10 && k<=99){
        lcd.print("B2: ");
          }
         else{
          lcd.print("B2:");
        }
        lcd.print(k);
  lcd.print("F");
  preferences.putInt("state2",k);
  
  delay(1000);
     }
}

if(ITS1C>=0 && ITS1C<=100 && ITS2C>=0 && ITS2C<=100 && p==1 && o==0)
{  
     if( timediff==0 && j==5 && n>l){
      lcd.clear();
     Serial.print("B1:");
     Serial.print(l);
     Serial.println("C");
     if(l>=0 && l<=9){
        lcd.print("B1:  ");
          }
        else if(l>=10 && l<=99){
        lcd.print("B1: ");
          }
         else{
          lcd.print("B1:");
        }
        lcd.print(l);
  lcd.print("C");
  preferences.putInt("state3",l);
  
  delay(1000);
  lcd.clear();
     Serial.print("B2:");
     Serial.print(n);
     Serial.println("C");
    if(n>=0 && n<=9){
        lcd.print("B2:  ");
          }
        else if(n>=10 && n<=99){
        lcd.print("B2: ");
          }
         else{
          lcd.print("B2:");
        }
        lcd.print(n);
  lcd.print("C");
  preferences.putInt("state4",n);
  
  delay(1000);
     }
}
     
     if(ITS1>=32 && ITS1<=212 && ITS2>=32 && ITS2<=212 && o==1 && p==0)
{
     if( timediff>=2000 && j==0 && k<=i)
     {
      lcd.clear();
        Serial.println("B1>=B2");
        lcd.print(" B1>=B2");
     delay(1000);
     lcd.clear();
     Serial.println("Inc B2");
     lcd.print("Inc B2");
     delay(1000);
     lcd.clear();
     Serial.print("B2:");
     Serial.print(k);
     Serial.println("F");
    if(k>=0 && k<=9){
        lcd.print("B2:  ");
          }
        else if(k>=10 && k<=99){
        lcd.print("B2: ");
          }
         else{
          lcd.print("B2:");
        }
        lcd.print(k);
  lcd.print("F");
  lcd.setCursor(3,0);
      lcd.blink();
      m=1;
  timediff=0;
     }
}
if(ITS1C>=0 && ITS1C<=100 && ITS2C>=0 && ITS2C<=100 && p==1 && o==0)
{
     if( timediff>=2000 && j==0 && n<=l)
     {
      lcd.clear();
        Serial.println("B1>=B2");
        lcd.print(" B1>=B2");
     delay(1000);
     lcd.clear();
     Serial.println("Inc B2");
     lcd.print("Inc B2");
     delay(1000);
     lcd.clear();
     Serial.print("B2:");
     Serial.print(n);
     Serial.println("C");
    if(n>=0 && n<=9){
        lcd.print("B2:  ");
          }
        else if(n>=10 && n<=99){
        lcd.print("B2: ");
          }
         else{
          lcd.print("B2:");
        }
        lcd.print(n);
  lcd.print("C");
  lcd.setCursor(3,0);
      lcd.blink();
      m=1;
  timediff=0;
     }
}
   
if( timediff>=500 && j==5  && m==5 &&  k>i && n>l ){
  
  lcd.clear();
  buttonpushed=1;
 /*val3=analogRead(analogPin3);
VM1=val3*3.3/(4095*MF);
Serial.print("VM1+: ");
Serial.println( VM1,3);

val4=analogRead(analogPin4);
VM11=val4*3.3/(4095*MF);
Serial.print("VM1-: ");
Serial.println( VM11);

V1=(VM1)-(VM11);
V2=(VM2)-(VM22);*/
val1=analogRead(analogPin1);
V1=(val1*3.3)/(4095*MF);
  ITS1C=(4*V1/0.00385)/(3.3-2*V1);
  ITS1=(9*ITS1C/5)+32;
  
  Serial.print("  V1:");
  Serial.println( V1,3);
  Serial.print("ITS1C: ");
  Serial.print( ITS1C);
  Serial.println( "C");
  Serial.print(" ITS1 :");
  Serial.print( ITS1);
  Serial.println( "F");
   Serial.println();
   if( o==1 && p==0){
  lcd.print("T1:");
  lcd.print(ITS1);
  lcd.print("F");
   
   if(ITS1>=i && ITS1<=k) {
    digitalWrite(Relay,HIGH);
  }
  else{
    digitalWrite(Relay,LOW);
  }
   }
    else if( p==1 && o==0){
  lcd.print("T1:");
  lcd.print(ITS1C);
  lcd.print("C");
   
   if(ITS1C>=l && ITS1C<=n) {
    digitalWrite(Relay,HIGH);
  }
  else{
    digitalWrite(Relay,LOW);
  }
   }
  
 delay(2000);
 
   val2=analogRead(analogPin2);
  V2=(val2*3.3)/(4095*MF);
  ITS2C=(4*V2/0.00385)/(3.3-2*V2);
  ITS2=(9*ITS2C/5)+32;
  Serial.print("  V2:");
  Serial.println( V2);
   Serial.print("ITS2C: ");
  Serial.print( ITS2C);
  Serial.println( "C");
   Serial.print(" ITS2 :");
  Serial.print( ITS2);
   Serial.println( "F");
   lcd.setCursor(0,1);
   
  if( o==1 && p==0){
    lcd.clear();
  lcd.print("T2:");
  lcd.print(ITS2);
  lcd.print("F");
   
   if(ITS2>=i && ITS2<=k) {
    digitalWrite(Pump,HIGH);
  }
  else{
    digitalWrite(Pump,LOW);
  }
   }
    else if( p==1 && o==0){
      lcd.clear();
  lcd.print("T2:");
  lcd.print(ITS2C);
  lcd.print("C");
   
   if(ITS2C>=l && ITS2C<=n) {
    digitalWrite(Pump,HIGH);
  }
  else{
    digitalWrite(Pump,LOW);
  }
   }
  
 delay(2000);
 
if(digitalRead(Relay)==1)
  {
    lcd.clear();
 lcd.print("RELAY ON");

 delay(2000);

 }
 else
 {
  lcd.clear();
  lcd.print("RELAYOFF");
   delay(2000);
}
 
  if(digitalRead(Pump)==1)
  {
 lcd.clear();
 lcd.print("PUMP ON");
delay(2000);

 }
 else
 {
 lcd.clear();
 lcd.print("PUMP OFF");
 delay(2000);
}
 }
}
  
void M1()
{  
if(digitalRead(button1)==0 && buttonpushed==0)
  {
    buttonpushed=millis();
    Serial.print("buttonpushed:");
    Serial.println(buttonpushed);
   }
   if(ITS1>=32 && ITS1<=212 && ITS2>=32 && ITS2<=212 && o==1 && p==0)
{
    if(digitalRead(button1)==0 && buttonpushed>0)
    {
    unsigned long difference=millis()-buttonpushed;
    if(difference>=event)
    {
      lcd.clear();
      Serial.print("difference:");
      Serial.println(difference);
     
     Serial.print("B1:");
     Serial.print(i);
      Serial.println("F");
      if(i>=0 && i<=9){
        lcd.print("B1:  ");
          }
         else if(i>=10 && i<=99){
        lcd.print("B1: ");
          }
         else{
          lcd.print("B1:");
        }
      lcd.print(i);
      lcd.print("F");
       lcd.setCursor(3,0);
      lcd.blink();
     
      Serial.println("Cursor4");
      j=1;
      m=0;
     buttonpushed=-1;
     Serial.print("buttonpushed:");
    Serial.println(buttonpushed);
    timediff=0;
      delay(3000);
    }
    }
}
    if(ITS1C>=0 && ITS1C<=100 && ITS2C>=0 && ITS2C<=100 && p==1 && o==0)
{
    if(digitalRead(button1)==0 && buttonpushed>0)
    {
    unsigned long difference=millis()-buttonpushed;
    if(difference>=event)
    {
      lcd.clear();
      Serial.print("difference:");
      Serial.println(difference);
     
     Serial.print("B1:");
     Serial.print(l);
      Serial.println("C");
      if(l>=0 && l<=9){
        lcd.print("B1:  ");
          }
         else if(l>=10 && l<=99){
        lcd.print("B1: ");
          }
         else{
          lcd.print("B1:");
        }
      lcd.print(l);
      lcd.print("C");
       lcd.setCursor(3,0);
      lcd.blink();
      
      Serial.println("Cursor4");
      j=1;
      m=0;
     buttonpushed=-1;
     Serial.print("buttonpushed:");
    Serial.println(buttonpushed);
    timediff=0;
      delay(3000);
    }
    }
}
}

void M2(){
  
 
     if(digitalRead(button2)==0 && flag1==0)
        {
          presstime=millis();
          flag1=1;
        Serial.print("presstime:");
    Serial.println(presstime);
        }
        
        if(digitalRead(button2)==1&& flag1==1)
        {
          releasetime=millis();
          flag1=0;
         
          Serial.print("releasetime:");
    Serial.println(releasetime);
    timediff=releasetime-presstime;
    Serial.print("timediff:");
    Serial.println(timediff);
        }
}
  
