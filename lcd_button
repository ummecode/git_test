       lcd.blink();
         
       delay(1000);
        timediff=0;
      }
}
}
      
      if(digitalRead(button1)==0&& j==3 && buttonpushed==-1&&m==0  )
      {
       lcd.setCursor(3,0);
       lcd.blink();
         Serial.println("Cursor4");
        j=1;
        delay(1000);
        }

        if(ITS1>=32 && ITS1<=212 && ITS2>=32 && ITS2<=212 && o==1 && p==0)
{  
        if( timediff>=2000 && m==0 && buttonpushed==-1)
        {
          lcd.clear();
        Serial.println("SAVE");
     lcd.print("  SAVE");
     delay(1000);
     lcd.clear();
     //digitalWrite(Relay,LOW);
     Serial.print("B2:");
     Serial.print(k);
     Serial.println("F");
      if(k>=0 && k<=9){
        lcd.print("B2:  ");
          }
