        lcd.print("B2: ");
          }
         else{
          lcd.print("B2:");
        }
        lcd.print(k);
      lcd.print("F");
      lcd.setCursor(3,0);
      lcd.blink();
      timediff=0;
    
      Serial.println("Cursor4");
      m=1;
      j=0;
     }
}

if(ITS1C>=0 && ITS1C<=100 && ITS2C>=0 && ITS2C<=100 && p==1 && o==0)
{  
        if( timediff>=2000 && m==0 && buttonpushed==-1)
        {
          lcd.clear();
        Serial.println("SAVE");
